{
  "manifest_version": 3,
  "name": "HideJobIn",
  "version": "1.0",
  "description": "Realmente oculta as vagas que foram definidas para ocultar.",
  "permissions": ["storage"],

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/jobs/search/*"],
      "js": ["js/hidejobin.js"]
    }
  ]
}
